<h1>Listing oasis_pages</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @oasis_pages.each do |oasis_page| %>
  <tr>
    <td><%= oasis_page.name %></td>
    <td><%= oasis_page.description %></td>
    <td><%= link_to 'Show', oasis_page %></td>
    <td><%= link_to 'Edit', edit_oasis_page_path(oasis_page) %></td>
    <td><%= link_to 'Destroy', oasis_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

